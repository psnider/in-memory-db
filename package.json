    "http-status-codes": "1.0.6",
    "pino": "3.0.1"
  },
  "devDependencies": {
    "@types/chai": "3.4.34",
    "@types/mocha": "2.2.33",
    "chai": "3.5.0",
    "mocha": "3.1.2",
    "typescript": "2.0.10"
  },
  "repository": {
